  "dependencies": {
    "get-stdin": "^8.0.0",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "eslint": "^8.1.0",
    "microbundle": "^0.13.0",
    "standard-version": "^9.0.0",
